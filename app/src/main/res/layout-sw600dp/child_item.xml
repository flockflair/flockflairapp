<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout4"
    tools:context=".ExpandableModule1"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/model"
        android:layout_width="800dp"
        android:layout_height="150dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_modules"
        android:gravity="center"
        android:layoutDirection="ltr"
        android:padding="15dp"
        android:paddingStart="70dp"
        android:paddingLeft="60dp"
        android:paddingEnd="40dp"
        android:paddingRight="10dp"
        android:text="Living world"
        android:textAlignment="center"
        android:textColor="#060C8B"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlHardcoded" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/childitem_image"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layoutDirection="ltr"
        android:src="@drawable/android_1"
        app:layout_constraintBottom_toBottomOf="@+id/model"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/model"
        app:layout_constraintVertical_bias="0.321"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/star"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="700dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="15dp"
        android:backgroundTintMode="multiply"
        android:foreground="@drawable/ic_star_xxhdpi_foreground"
        android:foregroundTint="@color/colorPrimary"
        android:layoutDirection="ltr"
        app:layout_constraintBottom_toBottomOf="@+id/model"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/model"
        app:layout_constraintVertical_bias="0.388"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/star_fav"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="700dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="15dp"
        android:backgroundTintMode="multiply"
        android:foreground="@drawable/ic_star_fav_xxhdpi_foreground"
        android:foregroundTint="@color/colorPrimary"
        android:layoutDirection="ltr"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/model"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/model"
        app:layout_constraintVertical_bias="0.388"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>